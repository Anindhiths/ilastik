[Core]
Name = CSV-Table
Module = tracking_csv_export

[Documentation]
Author = Carsten Haubold
Version = 0.1
Website = ilastik.org
Description = Plugin to export the ilastik tracking results to a CSV table <br> <br> <b>Usage: </b> Select the filename where the CSV table will be saved. <br> The resulting file will contain the lineageID and trackID of every object in the dataset, where valid track and lineage IDs <b>start from 2</b>. For now the set of exported features is predefined and cannot be changed.
